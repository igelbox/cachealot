{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "node",
        "request": "launch",
        "name": "Launch",
        "cwd": "${workspaceFolder}",
        "runtimeArgs": [
          "-r",
          "ts-node/register"
        ],
        "outputCapture": "std",
        "args": [
          "node_modules/jasmine/bin/jasmine",
          "src/*.spec.*ts",
        ],
        "skipFiles": [
          "<node_internals>/**/*.js",
        ],
        "env": {
        }
      }
    ]
  }
  